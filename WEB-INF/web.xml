    <servlet>
        <servlet-name>carttab</servlet-name>
        <servlet-class>ph.pup.itech.wucall.controller.carttab</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>li_su</servlet-name>
        <url-pattern>/LI-SU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
